{
  "name": "fetchum",
  "version": "4.0.0",
  "description": "A better fetch and local storage system",
  "main": "lib/index.js",
  "scripts": {
    "compile": "BABEL_ENV=npm babel src/ --out-dir lib",
    "prepublish": "npm run compile",
    "test": "BABEL_ENV=development jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jcgertig/fetchum.git"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/lib/"
    ]
  },
  "keywords": [
    "fetch",
    "api",
    "es6",
    "promise",
    "LocalStorage",
    "bearer",
    "Bearer",
    "JWT",
    "token"
  ],
  "author": "jcgertig <jcgertig@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jcgertig/fetchum/issues"
  },
  "homepage": "https://github.com/jcgertig/fetchum#readme",
  "dependencies": {
    "betterstorage": "^1.0.0",
    "core-decorators": "^0.20.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "memoizerific": "^1.11.2",
    "node-localstorage": "^1.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^20.0.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "jest": "^20.0.4",
    "regenerator-runtime": "^0.11.0"
  }
}
