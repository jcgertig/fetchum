{
  "env": {
    "development": {
      "presets": [
        "es2015",
        "stage-0"
      ],
      "plugins": [
        "transform-decorators-legacy",
        "transform-es2015-modules-umd"
      ]
    },
    "npm": {
      "presets": [
        "es2015",
        "stage-0"
      ],
      "plugins": [
        "transform-decorators-legacy",
        "transform-es2015-modules-umd"
      ]
    },
    "rollup": {
      "presets": [
        ["es2015", {
          "modules": false
        }],
        "stage-0"
      ],
      "plugins": [
        "transform-decorators-legacy",
        "external-helpers"
      ]
    }
  }
}
